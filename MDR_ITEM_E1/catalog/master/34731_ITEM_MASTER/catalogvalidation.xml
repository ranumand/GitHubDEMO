	<name>Catalog Validations</name>
	<description lang="en">Catalog Validations</description>
	<declare usage="regular">
		<var>primarydepartmentlink</var>
		<link type="catalog">
			<literal>PRIMARY_DEPARTMENT</literal>
			<literal>STATE/CONFIRMED</literal>
		</link>
	</declare>
	<declare usage="regular">
		<var>recapdepartmentlink</var>
		<link type="catalog">
			<literal>RECAP_DEPARTMENT</literal>
			<literal>STATE/CONFIRMED</literal>
		</link>
