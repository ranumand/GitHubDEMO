	<constraint>
		<name>AssignSubclassExtension</name>
		<description>tempvarforidext</description>
		<usefor/>
		<action>
			<assign>
				<var>subclass_extension</var>
				<op func="concat">
					<var>FAMILYTREECLASS</var>
					<const type="string">_</const>
					<var>FAMILYTREESUBDEPARTMENT</var>
				</op>
			</assign>
		</action>
	</constraint>
	<constraint>
		<name>AssignSubclassExtensionOverride</name>
		<description>tempvarforidext</description>
		<usefor/>
		<action>
			<assign>
				<var>subclass_extension_override</var>
				<op func="concat">
					<var>FAMILY_TREE_CLASS_CD</var>
					<const type="string">_</const>
					<var>FAMILY_TREE_DPT_CD</var>
				</op>
			</assign>
		</action>
	</constraint>
	<constraint>
		<name>DisplayPrimaryDepatmentName</name>
		<description>Displays primary department name for family tree reference data.</description>
		<usefor>
			<var>FAMILYTREEPRIMARYDEPARTMENT</var>
		</usefor>
		<action>
			<select novalue="default" showoninput="1,2">
				<table distinct="false" source="sql">
					<literal>primarydepartmentlink/PRODUCTID</literal>
					<literal>primarydepartmentlink/FAMILY_TREE_PRIMARY_DPT_NME</literal>
					<where type="sql">
						<sql>
							<eq>
								<var>FAMILYTREEPRIMARYDEPARTMENT</var>
								<literal>primarydepartmentlink/PRODUCTID</literal>
							</eq>
						</sql>
					</where>
				</table>
			</select>
		</action>
	</constraint>
	<constraint>
		<name>DisplayPrimaryDepatmentNameForOverride</name>
		<description>Displays primary department name for family tree reference data.</description>
		<usefor>
			<var>FAMILY_TREE_PRIMARY_DPT_CD</var>
		</usefor>
		<action>
			<select novalue="default" showoninput="1,2">
