					<literal>primarydepartmentlink/PRODUCTID</literal>
					<literal>primarydepartmentlink/FAMILY_TREE_PRIMARY_DPT_NME</literal>
					<where type="sql">
						<sql>
							<eq>
								<var>FAMILY_TREE_PRIMARY_DPT_CD</var>
								<literal>primarydepartmentlink/PRODUCTID</literal>
							</eq>
						</sql>
					</where>
				</table>
			</select>
		</action>
	</constraint>
