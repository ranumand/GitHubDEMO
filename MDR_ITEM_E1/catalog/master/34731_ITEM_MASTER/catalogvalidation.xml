	<constraint>
		<name>DisplayClassName</name>
		<description>Displays Class Name from Family tree.</description>
		<usefor>
			<var>FAMILYTREECLASS</var>
		</usefor>
		<action>
			<select novalue="default" showoninput="1,2">
				<table distinct="false" source="sql">
					<literal>classlink/PRODUCTID</literal>
					<literal>classlink/FAMILY_TREE_CLASS_NME</literal>
					<where type="sql">
						<sql>
							<and>
								<eq>
									<var>FAMILYTREECLASS</var>
									<literal>classlink/PRODUCTID</literal>
								</eq>
								<eq>
									<var>FAMILYTREESUBDEPARTMENT</var>
									<literal>classlink/PRODUCTIDEXT</literal>
								</eq>
							</and>
						</sql>
					</where>
				</table>
			</select>
		</action>
	</constraint>
	<constraint>
		<name>DisplayClassNameOverride</name>
		<description>Displays Class Name from Family tree.</description>
		<usefor>
			<var>FAMILY_TREE_CLASS_CD</var>
		</usefor>
